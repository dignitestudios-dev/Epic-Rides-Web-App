        {
          "name": "Verify OTP",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.environment.set(\"TOKEN\", pm.response.json().data.token)\r",
                  "\r",
                  "if(pm.response.json().data.user.role === 'driver'){\r",
                  "    pm.environment.set(\"DRIVER\", pm.response.json().data.token)\r",
                  "    return;\r",
                  "}\r",
                  "\r",
                  "pm.environment.set(\"USER\", pm.response.json().data.token)"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            },
            {
              "listen": "prerequest",
              "script": {
